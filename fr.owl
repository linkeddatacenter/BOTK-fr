                <owl:onClass rdf:resource="http://linkeddata.center/botk-fr/v1#FinancialReport"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A budget datasource document containing  goverment budget related  data.</rdfs:comment>
